{
  "name": "react-music-player",
  "version": "1.0.0",
  "description": "music player build by react",
  "main": "app/index.js",
  "scripts": {
    "start": "webpack-dev-server --inline --progress --port 8080 --host 0.0.0.0",
    "test": "karma start",
    "test:watch": "karma start --autoWatch=true --singleRun=false",
    "posttest": "npm run lint",
    "serve": "node server.js --env=dev",
    "serve:dist": "node server.js --env=dist",
    "dist": "npm run copy & webpack --env=dist",
    "lint": "eslint ./src",
    "copy": "copyfiles -f ./src/index.html ./src/favicon.ico ./dist",
    "clean": "rimraf dist/*",
    "release:major": "npm version major && npm publish && git push --follow-tags",
    "release:minor": "npm version minor && npm publish && git push --follow-tags",
    "release:patch": "npm version patch && npm publish && git push --follow-tags",
	"build":"...."
	
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/lvyidan/musicplayer-by-react.git"
  },
  "keywords": [
    "webpack",
    "react",
    "music-player"
  ],
  "author": "lyd",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/lvyidan/musicplayer-by-react/issues"
  },
  "homepage": "https://github.com/lvyidan/musicplayer-by-react#readme",
    "dependencies": {
    "pubsub-js": "^1.5.7",
    "react": "^15.6.1",
    "react-dom": "^15.6.1",
    "react-router": "^4.1.2",
    "react-router-dom": "^4.1.2"
  },
  "devDependencies": {
    "autoprefixer": "^7.1.2",
    "babel-core": "^6.25.0",
    "babel-loader": "^7.1.1",
    "babel-plugin-react-transform": "^2.0.2",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
	"copyfiles": "^1.0.0",
    "css-loader": "^0.23.0",
    "extract-text-webpack-plugin": "^3.0.0",
    "html-webpack-plugin": "^2.30.1",
    "json-loader": "^0.5.7",
    "node-sass": "^4.5.3",
    "react-hot-loader": "^3.0.0-beta.7",
    "sass": "^1.0.0-beta.2",
    "sass-loader": "^6.0.6",
    "style-loader": "^0.18.2",
     "webpack": "^3.5.1",
    "webpack-dev-server": "^2.7.0"
  }
}
